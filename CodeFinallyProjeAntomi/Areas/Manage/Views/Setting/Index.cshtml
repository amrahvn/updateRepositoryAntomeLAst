@model IEnumerable<Setting>
@{
    ViewData["Title"] = "Index";
}


<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h3>Setting</h3>
    <a asp-area="manage" asp-controller="Setting" asp-action="Create" class="d-sm-inline-block btn btn-sm btn-primary shadow-sm" style="position: absolute;
    right: 72px;
    top: 6rem;">
        <i class="fas fa-download fa-smile-o text-white-50"></i> Create
    </a>
</div>

<div class="row">
    <div class="container">


        <table class="table table-striped table-dark" style="
    margin-left: 138px;
    margin-top: 8rem;
">

            <thead>

                <tr>
                    <th scope="col">Key</th>
                    <th scope="col">Value</th>
                </tr>
            </thead>
            <tbody>
                @foreach (Setting setting in Model)
                {
                    <tr>
                        <td>@setting.Key</td>
                        <td>@setting.Value</td>
                        <td>
                            <a class="btn btn-warning" asp-area="manage" asp-controller="Setting" asp-action="Update" asp-route-id="@setting.Id">Update</a>
                            <a class="btn btn-primary" asp-area="manage" asp-controller="Setting" asp-action="Delete" asp-route-id="@setting.Id">Delete</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>



